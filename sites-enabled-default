Listen 8080
<VirtualHost *:8080> 
    ServerName ack-default  
    DocumentRoot /projects/ack_default/public 
    SetEnv APPLICATION_ENV "development" 
    
    <Directory /projects/ack_default/public > 
        DirectoryIndex index.php 
        AllowOverride All 
        Order allow,deny 
        Allow from all 
    </Directory> 
</VirtualHost>


